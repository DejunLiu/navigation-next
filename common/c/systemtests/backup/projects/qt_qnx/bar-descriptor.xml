    <configuration name="Default">
       <platformArchitecture>armle-v7</platformArchitecture>
       <asset path="arm/o.le-v7-g/systemtests" entry="true" type="Qnx/Elf">systemtests</asset>
    </configuration>
	
    <configuration name="Device-Debug">
       <platformArchitecture>armle-v7</platformArchitecture>
       <asset path="arm/o.le-v7-g/systemtests" entry="true" type="Qnx/Elf">systemtests</asset>
	   <asset path="${MVC_SDK_ARM_LIB}/libRenderEngine3d.so" type="Qnx/Elf">lib/libRenderEngine3d.so</asset>
    </configuration>
    
	<configuration name="Device-Release">
       <platformArchitecture>armle-v7</platformArchitecture>
       <asset path="arm/o.le-v7/systemtests" entry="true" type="Qnx/Elf">systemtests</asset>
	   <asset path="${MVC_SDK_ARM_LIB}/libRenderEngine3d.so" type="Qnx/Elf">lib/libRenderEngine3d.so</asset>
    </configuration>
    
	<configuration name="Simulator-Debug">
       <platformArchitecture>x86</platformArchitecture>
       <asset path="x86/o-g/systemtests" entry="true" type="Qnx/Elf">systemtests</asset>
	   <asset path="${MVC_SDK_X86_LIB}/libRenderEngine3d.so" type="Qnx/Elf">lib/libRenderEngine3d.so</asset>
    </configuration>
    
	<configuration name="Simulator-Release">
