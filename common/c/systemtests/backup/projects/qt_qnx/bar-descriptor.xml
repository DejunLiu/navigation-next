    <asset path="$(PROJECT_LOC)/../../../supportfiles/batchtool/bt_route_in.csv">../../bt_route_in.csv</asset>
    <asset path="${QNX_TARGET}\${CPUVARDIR}\usr\lib\qt4\lib\libQtGui.so" type="Qnx/Elf">lib/libQtGui.so.4</asset>
    <asset path="icon.png">icon.png</asset>
